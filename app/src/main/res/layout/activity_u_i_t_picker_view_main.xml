<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui_third.WheelView.UITPickerViewMainActivity">


    <com.contrarywind.view.WheelView
        android:id="@+id/wheelView4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/wheelView3"
        app:layout_constraintTop_toTopOf="@+id/wheelView3" />

    <com.contrarywind.view.WheelView
        android:id="@+id/wheelView1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toStartOf="@+id/wheelView2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.contrarywind.view.WheelView
        android:id="@+id/wheelView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/wheelView3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/wheelView1"
        app:layout_constraintTop_toTopOf="@+id/wheelView1" />

    <com.contrarywind.view.WheelView
        android:id="@+id/wheelView3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/wheelView4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/wheelView2"
        app:layout_constraintTop_toTopOf="@+id/wheelView2" />
</androidx.constraintlayout.widget.ConstraintLayout>